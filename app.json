{
  "expo": {
    "name": "zewst",
    "slug": "zewstapp",
    "version": "1.0.51",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "This app uses the location to find the Person near you.",
        "NSLocationWhenInUseUsageDescription": "This app uses the location to find the Person near you.",
        "NSCameraUsageDescription": "This app uses the camera to take your profile picture.",
        "NSPhotoLibraryUsageDescription": "This app uses Photo Library for uploading your profile picture.",
        "UIBackgroundModes": ["remote-notification"]
      },
      "bundleIdentifier": "com.hauraki.zewst",
      "splash": {
        "tabletImage": "./assets/tabletSplash.png"
      },
      "config": {
        "googleMapsApiKey": "AIzaSyAGC7lHJNpuBGN4RCobhFeTkcCL_mX5_BY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#A461D8"
      },
      "package": "com.hauraki.zewst",
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAGC7lHJNpuBGN4RCobhFeTkcCL_mX5_BY"
        }
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "userInterfaceStyle": "automatic"
  },
  "plugins": [
    [
      "expo-notifications",
      {
        "icon": "./assets/icon.png",
        "sounds": ["./assets/notification.wav"]
      }
    ]
  ]
}
